<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns0="http://enterprise.netbeans.org/bpel/TaxiService/taxiService" xmlns:ns1="http://j2ee.netbeans.org/wsdl/TaxiService/TaxiService" xmlns:ns2="http://j2ee.netbeans.org/wsdl/TaxiService/TaxiRequest" xmlns:ns3="http://www.sun.com/jbi/descriptor/service-unit" version="1.0">
    <services binding-component="false">
        <provides endpoint-name="TaxiRequestPortTypeRole_myRole" interface-name="ns2:TaxiRequestPortType" service-name="ns0:ProcessManager">
            <ns3:display-name>ProcessManager</ns3:display-name>
            <ns3:process-name>taxiService</ns3:process-name>
            <ns3:file-path>taxiService.bpel</ns3:file-path>
        </provides>
        <consumes endpoint-name="jdbcPortTypeRole_partnerRole" interface-name="ns1:jdbcPortType" service-name="ns0:TaxiDatabase">
            <ns3:display-name>TaxiDatabase</ns3:display-name>
            <ns3:process-name>taxiService</ns3:process-name>
            <ns3:file-path>taxiService.bpel</ns3:file-path>
        </consumes>
    </services>
</jbi>
