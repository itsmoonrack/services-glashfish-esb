<?xml version="1.0" encoding="UTF-8"?>
<process
    name="hotelReception"
    targetNamespace="http://enterprise.netbeans.org/bpel/HotelReception/hotelReception"
    xmlns:tns="http://enterprise.netbeans.org/bpel/HotelReception/hotelReception"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
   <import namespace="http://j2ee.netbeans.org/wsdl/HotelReception/ScubaDivingRequest" location="ScubaDivingRequest.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
   <partnerLinks>
      <partnerLink name="ProcessManager" xmlns:tns="http://j2ee.netbeans.org/wsdl/HotelReception/ScubaDivingRequest" partnerLinkType="tns:ScubaDivingRequest" partnerRole="ScubaDivingRequestPortTypeRole"/>
   </partnerLinks>
   <sequence>
      <receive name="GetClientReservation" createInstance="yes"/>
   </sequence>
</process>

